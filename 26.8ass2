#include <stdio.h>
#include<string.h>
int main()
{
    char s[50];
    int i,j;
    printf("Enter a string:");
    gets(s);
    int n=strlen(s);
    i=0,j=n-1;
    while(i<(n/2))
    {
        if(s[i]==s[j])
        {
            i++;
            j--;
        }
        
    }
    if(s[i]==s[j])
        printf("palindrome");
    else
        printf("not a palindrome");
    return 0;
}
